{
    "name":"Invert.io",
    "description":"Go from Dark mode to light mode and vice versa",
    "version":"1.0.0",
    "manifest_version":2,
    "background":{
        "scripts":[
                  "popup.js"
        ],
       "persisten": true
    },
   "browser_action":{
        "default_title":"Invert.io",
        "default_popup":"popup.html"
   },
   "permissions":[
       "https://*/*",
       "http://*/*",
       "tabs"
   ]
}